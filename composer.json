{
    "name": "xtcat/bus",
    "description": "An improved command bus for Laravel",
    "keywords": ["bus", "command", "job", "command bus", "Bus", "Alt Three"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alt Three",
            "email": "support@alt-three.com"
        }
    ],
    "require": {
        "php": "^8.2 || ^8.3",
        "illuminate/bus": "^11.0",
        "illuminate/container": "^11.0",
        "illuminate/contracts": "^11.0",
        "illuminate/pipeline": "^11.0",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.4 || ^3.0",
        "graham-campbell/testbench": "^5.5",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^6.5 || ^7.5 || ^8.4 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "AltThree\\Bus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AltThree\\Tests\\Bus\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
